<!DOCTYPE html>
<html>
  <head>
    <title>MvcExercise</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <% if user_signed_in? %>
    User Signed In
    <%end%>

    <% if admin_signed_in? %>
    Admin Signed In
    <%end%>


    <%= yield %>
  </body>
</html>
